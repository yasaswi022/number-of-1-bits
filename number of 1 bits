#include<stdio.h>
int main()
{
    int t,n;       
    scanf("%d",&t);
        while(t--)
{
        scanf("%d",&n);
        int result=0;
        while(n)
{
        int r=n%2;
        if(r==1)
{
            result =result+1;
        }
        n=n/2;          
        }
        printf("%d\n",result);
        
    }
}
